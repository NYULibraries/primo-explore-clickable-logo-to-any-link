{
  "name": "primo-explore-clickable-logo-to-any-link",
  "version": "1.1.8",
  "description": "ExLibris Primo New UI plugin for creating a clickable logo that goes to any configurable URL",
  "main": "./dist/index.js",
  "scripts": {
    "test": "NODE_ENV=test yarn karma start --browsers=ChromeHeadless --single-run",
    "test:chrome-debugger": "yarn karma start --browsers=Chrome --single-run=false --debug",
    "build": "webpack --mode=production",
    "prepare": "rimraf dist/* && yarn build",
    "publish-dry-run": "yarn prepare && yarn pack --filename primo-explore-clickable-logo-to-any-link.tgz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyulibraries/primo-explore-clickable-logo-to-any-link.git"
  },
  "keywords": [
    "primo-explore"
  ],
  "author": "Barnaby Alter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nyulibraries/primo-explore-clickable-logo-to-any-link/issues"
  },
  "homepage": "https://github.com/nyulibraries/primo-explore-clickable-logo-to-any-link#readme",
  "devDependencies": {
    "@babel/core": "7.5.0",
    "@babel/preset-env": "7.5.0",
    "angular": "1.6.10",
    "angular-mocks": "1.6.10",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "jasmine-core": "^3.3.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-coveralls": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^5.0.0-alpha.3.0",
    "puppeteer": "^5.5.0",
    "rimraf": "^2.6.3",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.1.0"
  }
}
